fn print_length(list<string> l) -> int {
    int d = l.length();
    print("The first element is %s \n", l.get(0) );
    return d;
}

list<string> names = ["manish", "lan", "gourley", "unan"];
int l = print_length(names);
int new_l = l - 1;
print("The length is: %d\n", l);
print("The last element is: %s\n", names.get(new_l));