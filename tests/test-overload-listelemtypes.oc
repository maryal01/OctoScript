fn overload max (list<int> li) -> int {
    int max = li.list_get(0);
    int i = 1;
    while (i < li.list_length()) {
        int v = li.list_get(i);
        if (v > max){
            max = v; 
        }
        i = i + 1;
    }
    return max;
}

fn overload max (list<float> lf) -> float {
    float max = lf.list_get(0);
    int i = 1;
    while (i < lf.list_length()) {
        float v = lf.list_get(i);
        if (v > max){
            max = v; 
        }
        i = i + 1;
    }
    return max;
}

list<int> ilist = [13, 79, 94, 20, 39, 82, 23];
list<float> flist = [83.82, 58.33, 78.19, 4.48, 56.59, 79.70, 18.88];

print("max of int list: %d; max of float list %.2f\n", max(ilist), max(flist));
/* prints: max of int list: 94; max of float list 83.82 */